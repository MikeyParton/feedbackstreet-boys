{
  "manifest_version": 2,
  "name": "Name of your chrome extension",
  "description": "Description of your chrome extension.",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": []
}