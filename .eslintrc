{
  "env": {
    "commonjs": true,
    "es6": true,
    "browser": true,
    "webextensions": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "prettier",
    "import",
    "jsx-a11y",
    "react",
    "react-hooks"
  ],
  "rules": {
    "no-console": "warn",
    "react/jsx-props-no-spreading": "off",
    "react/prop-types": "off",
    "import/prefer-default-export": "off"
  }
}